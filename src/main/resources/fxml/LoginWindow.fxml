<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane maxHeight="392.0" maxWidth="340.0" minHeight="240.0" minWidth="315.0" prefHeight="288.0" prefWidth="315.0" styleClass="main-pane" stylesheets="@../css/LoginWindow.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.milbar.gui.LoginWindowController">
    <children>
        <Label layoutX="111.0" layoutY="21.0" prefHeight="62.0" prefWidth="193.0" styleClass="big-label" stylesheets="@../css/LoginWindow.css" text="Log in" textAlignment="CENTER">
            <font>
                <Font name="Times New Roman" size="28.0" />
            </font></Label>
        <TextField fx:id="usernameField" layoutX="98.0" layoutY="100.0" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="98.0" layoutY="137.0" onAction="#loginButtonClicked" promptText="Password" styleClass="text-field" />
        <Button layoutX="161.0" layoutY="198.0" mnemonicParsing="false" onAction="#loginButtonClicked" prefHeight="30.0" prefWidth="113.0" styleClass="button" stylesheets="@../css/LoginWindow.css" text="Log in" />
        <Button layoutX="67.0" layoutY="198.0" mnemonicParsing="false" onAction="#cancelButtonClicked" prefHeight="30.0" prefWidth="77.0" styleClass="button" stylesheets="@../css/LoginWindow.css" text="Cancel" />
        <Label fx:id="errorLabel" layoutX="73.0" layoutY="175.0" prefHeight="17.0" prefWidth="177.0" styleClass="error-label" stylesheets="@../css/LoginWindow.css" textAlignment="CENTER" textFill="#ff5300" />
        <ImageView fitHeight="30.0" fitWidth="34.0" layoutX="67.0" layoutY="97.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../images/login.png" />
        </ImageView>
        <ImageView fitHeight="30.0" fitWidth="34.0" layoutX="67.0" layoutY="137.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../images/password.png" />
        </ImageView>
      <Button layoutX="66.0" layoutY="236.0" mnemonicParsing="false" onAction="#registerButtonClicked" styleClass="register-button" stylesheets="@../css/LoginWindow.css" text="Don't have an account? Register here" />
    </children>
</AnchorPane>
